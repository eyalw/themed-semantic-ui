{
  "base": "semantic",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "customize/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "rtl": "No",
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "flag",
    "header",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "segment",
    "form",
    "grid",
    "message",
    "table",
    "item",
    "checkbox",
    "dimmer",
    "dropdown",
    "modal",
    "popup",
    "search",
    "api",
    "icon",
    "transition",
    "form",
    "menu",
    "state"
  ],
  "ignore": [
    "reveal",
    "shape",
    "tab",
    "feed",
    "comment",
    "rail",
    "sticky",
    "step",
    "breadcrumb",
    "card",
    "progress",
    "accordion",
    "statistic",
    "embed",
    "visibility",
    "sidebar"
  ],
  "version": "2.1.8"
}
